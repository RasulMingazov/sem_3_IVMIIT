<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="615.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.classwork.FirstController">
   <children>
      <BarChart fx:id="bar" barGap="2.0" categoryGap="5.0" layoutX="3.0" prefHeight="265.0" prefWidth="618.0" title="Person">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <LineChart fx:id="lineChart" layoutX="-1.0" layoutY="263.0" prefHeight="271.0" prefWidth="624.0" title="Chart">
        <xAxis>
          <NumberAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="505.0" layoutY="551.0" mnemonicParsing="false" onAction="#onNextTaskClick" prefHeight="27.0" prefWidth="96.0" text="Далее" />
   </children>
</AnchorPane>
